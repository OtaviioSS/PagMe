        <activity
            android:name=".presentation.activities.ProfileUserActivity"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity android:name=".presentation.activities.DetailDebtActivity" />
        <activity android:name=".presentation.activities.LoginActivity" />
        <activity android:name=".presentation.activities.FormUserActivity" />
        <activity
            android:name=".presentation.activities.ListDebtActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".presentation.activities.FormDebtActivity" />
