        android:supportsRtl="true"
        android:theme="@style/Theme.Pagme">
        <activity
            android:name=".presentation.activities.ListCardActivity"
            android:exported="false" />
        <activity
            android:name=".presentation.activities.VerifiedActivity"
            android:exported="false" />
        <activity
            android:name=".presentation.activities.ForgetPasswordActivity"
